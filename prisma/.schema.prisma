generator client {
  provider   = "prisma-client"
  output     = "../prisma-client"
  engineType = "client"
}

datasource db {
  provider = "mysql"
  // url      = env("DATABASE_URL")
}


model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  // posts Post[]
}
